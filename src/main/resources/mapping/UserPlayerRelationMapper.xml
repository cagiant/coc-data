<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.coc.data.mapper.UserPlayerRelationMapper">
    <select id="getUserPlayerRelation" resultMap="BaseResultMap">
        select * from user_player_relation where user_id = #{userId} and player_id = #{playerId} and is_deleted = 0
    </select>
</mapper>